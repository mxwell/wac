{
  "templates_dir": "/home/murat/.config/wac/templates",
  "default_template": "gcc",
  "solution_name": "main",
  "extensions": {
    "c++11": "cpp",
    "python3": "py"
  },
  "build_methods": {
    "gcc": {
      "language": "c++11",
      "command": "g++ --std=c++11 -pedantic -Wshadow -Wformat=2 -Wfloat-equal -Wconversion -Wlogical-op -fwhole-program -fsanitize=address -fstack-protector -Wall -Werror -Wextra -o $OUTPUT $INPUT"
    },
    "gcc_fast": {
      "language": "c++11",
      "command": "g++ --std=c++11 -o $OUTPUT $INPUT"
    },
    "python3": {
      "language": "python3",
      "command": "cp $INPUT $OUTPUT"
    }
  },
  "default_build_method": "gcc",
  "exec_methods": {
    "gcc": {
      "command": "./$OUTPUT"
    },
    "python3": {
      "command": "python3 $OUTPUT"
    }
  },
  "default_exec_method": "gcc"
}
